  	<servlet-class>login.login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>cser</servlet-name>
  	<display-name>cser</display-name>
  	<description></description>
  	<servlet-class>customer.cser</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ureg</servlet-name>
  	<url-pattern>/ureg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>cser</servlet-name>
  	<url-pattern>/cser</url-pattern>
  </servlet-mapping>
</web-app>
